<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper 
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.innilabs.board.mapper.UserMapper"> 
    <select id="selectUserByUserId" parameterType="com.innilabs.board.dto.User" resultType="com.innilabs.board.dto.User">
        SELECT 
            *
        FROM 
            USER
        WHERE
            user_id = #{userId}
    </select> 
</mapper>